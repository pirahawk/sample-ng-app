@page
@model IndexModel
@{
    ViewData["Title"] = "Zoopla Agent Portal";
}

<header>
    <app-navigation></app-navigation>
</header>
<app-root></app-root>


@section Scripts
{
    @if (Model.ShouldIncludeEs2015Modules())
    {
        <script src="~/js/portalClient/runtime-es2015.js" type="module"></script>
        <script src="~/js/portalClient/runtime-es5.js" nomodule defer></script>
        <script src="~/js/portalClient/polyfills-es5.js" nomodule defer></script>
        <script src="~/js/portalClient/polyfills-es2015.js" type="module"></script>
        <script src="~/js/portalClient/vendor-es2015.js" type="module"></script>
        <script src="~/js/portalClient/vendor-es5.js" nomodule defer></script>
        <script src="~/js/portalClient/main-es2015.js" type="module"></script>
        <script src="~/js/portalClient/main-es5.js" nomodule defer></script>
    }
    else
    {
        <script src="~/js/portalClient/main.js"></script>
        <script src="~/js/portalClient/polyfills.js"></script>
        <script src="~/js/portalClient/runtime.js"></script>
        <script src="~/js/portalClient/vendor.js"></script>
    }

}
